   "source": [
    "def factorial(n: int) -> int:\n",
    "    if n == 1 or n == 0:\n",
    "        return 1\n",
    "    else:\n",
    "        return n * (factorial(n-1))\n",
    "   \n",
    "print(factorial(6))"
   ],
